{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "*": ["types/*"]
    }
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
